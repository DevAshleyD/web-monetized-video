{
  "name": "web-monetization-video",
  "version": "0.0.3",
  "description": "A video web component which charges you the amount for which you have watched the video.",
  "main": "dist/wm-ad-hider.cjs.js",
  "scripts": {
    "build": "rollup -c && npm run minify",
    "dev": "rollup -c -w",
    "minify": "uglifyjs dist/web-monetized-video.umd.js --compress --output dist/web-monetized-video.min.js",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browser": "dist/web-monetized-video.umd.js",
  "module": "dist/web-monetized-video.es.js",
  "unpkg": "dist/web-monetized-video.min.js",
  "files": [
    "src/web-monetized-video.js",
    "demo/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jasmin2895/web-monetized-video.git"
  },
  "keywords": [
    "web monetization",
    "video",
    "custom element",
    "web component"
  ],
  "author": "Jasmin Virdi <jasmin28virdi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jasmin2895/web-monetized-video/issues"
  },
  "homepage": "https://github.com/Jasmin2895/web-monetized-video#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.0-beta",
    "rollup": "^2.10.2",
    "rollup-plugin-babel": "^4.4.0",
    "uglify-js": "^3.9.3"
  }
}
